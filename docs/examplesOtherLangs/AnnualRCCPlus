#include <iostream>
#include <cmath>

int main() {
    double initial = 10000.0;
    double final = 15000.0;
    double years = 3.0;
    double annualReturn = pow(final / initial, 1.0 / years) - 1;
    std::cout << "Annualized Return: " << annualReturn << std::endl;
    return 0;
}

